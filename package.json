{
  "name": "prueba-tecnica",
  "version": "1.0.0",
  "description": "Full stack application",
  "private": true,
  "scripts": {
    "install:all": "npm install && npm install --workspace=backend && npm install --workspace=frontend",
    "ci": "npm run lint && npm run test && npm run build",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "npm run lint --workspace=backend",
    "lint:backend:check": "npm run lint:check --workspace=backend",
    "lint:frontend": "npm run lint --workspace=frontend",
    "lint:frontend:check": "npm run lint:check --workspace=frontend",
    "format": "npm run format:backend && npm run format:frontend",
    "format:backend": "npm run format --workspace=backend",
    "format:frontend": "npm run format --workspace=frontend",
    "format:check": "npm run format:backend:check && npm run format:frontend:check",
    "format:backend:check": "npm run format:check --workspace=backend",
    "format:frontend:check": "npm run format:check --workspace=frontend",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "npm run test --workspace=backend",
    "test:frontend": "npm run test --workspace=frontend",
    "test:frontend:ui": "npm run test:ui --workspace=frontend",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "npm run build --workspace=backend",
    "build:frontend": "npm run build --workspace=frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "start": "npm run start --workspace=backend",
    "preview": "npm run preview --workspace=frontend",
    "migrate": "npm run migrate --workspace=backend",
    "seed": "npm run seed --workspace=backend",
    "clean": "npm run clean:backend && npm run clean:frontend",
    "clean:backend": "rimraf backend/dist",
    "clean:frontend": "rimraf frontend/dist"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "workspaces": [
    "backend",
    "frontend"
  ]
}
